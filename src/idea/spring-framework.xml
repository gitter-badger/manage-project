					</match>
				</rule>
			</rules>
		</arrangement>
	</codeStyleSettings>
	<codeStyleSettings language="XML">
		<indentOptions>
			<option name="USE_TAB_CHARACTER" value="true"/>
		</indentOptions>
	</codeStyleSettings>
